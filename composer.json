{
  "require": {
    "php": ">=7.3",
    "ext-fileinfo": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/functions.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "composer dump-autoload"
    ],
    "post-update-cmd": [
      "composer dump-autoload"
    ]
  },
  "require-dev": {
    "symfony/var-dumper": "^5.4"
  }
}
